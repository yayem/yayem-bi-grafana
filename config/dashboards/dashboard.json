          },
          "queryType": "sql",
          "rawSql": "SELECT toDate(synchronizationDate), COUNT(status), status \nFROM \"membersInTime\" \nWHERE IF('${status2}'='all', 1=1, if('${status2}'='total_members', status in ('active', 'paused', 'past_due'), status in ('guest', 'new', 'awaiting_review', 'approved'))) \nAND IF('${subscriptionProductId}'='all', 1=1, if('${subscriptionProductId}'='other', subscriptionProductId NOT IN ('prod_N8Lc8Q5gw9VXRs', 'prod_MyyEPECet88fTN'), subscriptionProductId='${subscriptionProductId}'))\nAND IF('${country}'='all', 1=1, countryOfResidence='${country}')\nGROUP BY toDate(synchronizationDate), status \nORDER BY toDate(synchronizationDate)",
          "refId": "A",
          "selectedFormat": 0
        },
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PFEF3D75BD8706C16"
          },
          "hide": false,
          "meta": {
            "builderOptions": {
              "fields": [],
              "limit": 100,
              "mode": "list"
            }
          },
          "queryType": "sql",
          "rawSql": "SELECT\n    toDate(synchronizationDate) AS sync_date,\n    CASE\n        WHEN status IN ('guest', 'new', 'awaiting_review', 'approved') THEN 'potential'\n        WHEN status IN ('active', 'paused', 'past_due') THEN 'members'\n        ELSE status\n    END AS grouped_status,\n    COUNT(*) AS total_count\nFROM membersInTime\nWHERE IF('${status2}'='all', grouped_status IN ('potential', 'members'), if('${status2}'='total_members', grouped_status='members', grouped_status='potential'))\nAND IF('${subscriptionProductId}'='all', 1=1, if('${subscriptionProductId}'='other', subscriptionProductId NOT IN ('prod_N8Lc8Q5gw9VXRs', 'prod_MyyEPECet88fTN'), subscriptionProductId='${subscriptionProductId}'))\nAND IF('${country}'='all', 1=1, countryOfResidence='${country}')\nGROUP BY sync_date, grouped_status\nORDER BY sync_date, grouped_status;",
          "refId": "B"
        }
      ],
      "title": "Users In Time",
      "type": "timeseries"
    },
